{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      "no-console": "off",
      "func-names": "off",
      "no-process-exit": "off",
      "object-shorthand": "off",
      "class-methods-use-this": "off",
      "import/no-unresolved": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "settings": {
      "import/resolver": {
        "node": {
        "extensions": [".js", ".jsx", ".json", ".native.js"]
        }
      }
    }
  }